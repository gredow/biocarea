<Application x:Class="BioCarea.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BioCarea"
             >
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="imgBorrar" UriSource="/recursos/img/borrar.png" />
            <Image x:Key="imgGuardar" Source="/recursos/img/guardar.png" />
            <ResourceDictionary.MergedDictionaries >
                <ResourceDictionary>
                    <local:Bootstrapper x:Key="Bootstrapper"/>
                </ResourceDictionary>
                <ResourceDictionary Source="recursos/style/EstiloGeneral.xaml"/>
            </ResourceDictionary.MergedDictionaries >
        </ResourceDictionary>
    </Application.Resources>
</Application>
